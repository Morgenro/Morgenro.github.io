---
import BaseHead from '../components/BaseHead.astro';
import Footer from '../components/Footer.astro';
import Header from '../components/Header.astro';
---

<!doctype html>
<html lang="zh-CN">
  <head>
    <BaseHead title="日记" description="个人日记" />
    <style>
      main {
        width: 720px;
        max-width: calc(100% - 2em);
        margin: 2em auto;
        padding: 0 1em;
      }
      .diary-timeline {
        position: relative;
        padding-left: 2em;
      }
      .diary-timeline::before {
        content: '';
        position: absolute;
        left: 0.5em;
        top: 0;
        bottom: 0;
        width: 2px;
        background: #e0e0e0;
      }
      .diary-item {
        position: relative;
        margin-bottom: 2em;
        padding: 1em 1.2em;
        background: #fff;
        border: 1px solid #e8e8e8;
        border-radius: 8px;
        cursor: pointer;
        transition: box-shadow 0.2s ease;
      }
      .diary-item::before {
        content: '';
        position: absolute;
        left: -1.75em;
        top: 1.2em;
        width: 10px;
        height: 10px;
        border-radius: 50%;
        background: #888;
        border: 2px solid #fff;
      }
      .diary-item:hover {
        box-shadow: 0 2px 12px rgba(0,0,0,0.1);
      }
      .diary-date {
        font-size: 0.85em;
        color: #888;
        margin-bottom: 0.3em;
      }
      .diary-title {
        font-size: 1.1em;
        font-weight: bold;
        margin-bottom: 0.6em;
        color: #333;
      }
      .diary-content {
        font-size: 0.95em;
        color: #555;
        line-height: 1.7;
      }
      .diary-content p {
        margin: 0.4em 0;
      }
      .giscus-container {
        margin-top: 1.5em;
        padding-top: 1em;
        border-top: 1px solid #eee;
      }
    </style>
  </head>
  <body>
    <Header />
    <main>
      <h1>日记</h1>
      <div class="diary-timeline">
        <div class="diary-item" data-term="diary-2025-10-08">
